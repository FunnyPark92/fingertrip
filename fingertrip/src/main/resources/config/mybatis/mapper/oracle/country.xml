<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="config.mybatis.mapper.oracle.country">
	<select id="selectAllCountry" resultType="CountryVO">
		SELECT * FROM COUNTRY
	</select>
	
	<select id="selectSomeCountry" parameterType="int" resultType="CountryVO">
		SELECT * FROM COUNTRY WHERE CONTINENT_NO=#{CONTINENT_NO}
	</select>
	
</mapper>









